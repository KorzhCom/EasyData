@page "/orders"
@model EasyDataBasicDemo.Pages.OrdersModel
@{
    ViewData["Title"] = "Entities";
}

<div id="EasyDataContainer"></div>

@section Scripts {
    @* <link rel="stylesheet" href="/css/easydata.css" /> *@
    <script src="/js/easydata.js" type="text/javascript"></script>

    <script>
        window.addEventListener('load', function () {
            easydata.runDispatcher({
                endpoint: '/api/easy-crud',
                rootEntity: 'Order'
            })
        });
    </script>
}