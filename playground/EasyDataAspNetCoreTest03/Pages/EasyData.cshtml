}

<link rel="stylesheet" href="/css/easydata.min.css" />
<!--
<link rel="stylesheet" href="https://cdn.korzh.com/ed/1.4.20/easydata.min.css" />
-->

<div id="EasyDataContainer"></div>

@section Scripts {
    <script src="/js/easydata.min.js" type="text/javascript"></script>
    <!--
    <script src="https://cdn.korzh.com/ed/1.4.20/easydata.min.js" type="text/javascript"></script>
    -->
    <script>
        window.addEventListener('load', function () {
            new easydata.crud.EasyDataViewDispatcher().run()
        });
    </script>
}